package Praktikums1;

import java.util.Scanner;

public class Praktikum44 {

    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        String jawaban1;
        System.out.println("Sebutkan salah satu unsur terpenting dalam sejarah...");
        System.out.print("KETIK JAWABAN ANDA : ");
        jawaban1 = in.nextLine();

        switch (jawaban1) {
            case "manusia":

            case "ruang":

            case "waktu":
                System.out.println(jawaban1 + " merupakan salah satu unsur terpenting pembentuk sejarah");
                break;

            default:
                System.out.println(jawaban1 + " bukanlah salah satu unsur terpenting pembentuk sejarah");
        }
    }

}
